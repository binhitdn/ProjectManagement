{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@api/*": ["./src/api/*"],
      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@constants/*": ["./src/constants/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@navigation/*": ["./src/navigation/*"],
      "@redux/*": ["./src/redux/*"],
      "@screens/*": ["./src/screens/*"],
      "@styles/*": ["src/styles/*"],
      "@utils/*": ["./src/utils/*"],
      "@axios/*": ["./src/axios.js"],
      "@helpers/*": ["./src/helpers/*"],
      "@customize/*": ["./src/components/customize/*"]
    }
  }
}
